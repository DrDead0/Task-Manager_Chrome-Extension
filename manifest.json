
{
  "manifest_version": 3,
  "name": "Task Manager",
  "version": "3.0",
  "description": "A task manager extension",
  "permissions": ["storage", "alarms", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "options.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  }
}
